{   
    "parser": "babel-eslint",
    "extends": ["plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "strict": 0,
        "semi": 2,
        "arrow-parens": 0
    },
    "globals": {
        "document": true,
        "window": true
    }
}
